import { createRouter, createWebHistory } from 'vue-router';
import Beranda from '../pages/Beranda.vue';
import PenambanganData from '../pages/PenambanganData.vue';
import SainsData from '../pages/SainsData.vue';
import AnalisisData from '../pages/AnalisisData.vue';
import Proyek from '../pages/Proyek.vue';
import Prediksi from '../pages/Prediksi.vue';
import Chatbot from '../pages/Chatbot.vue';
import Tentang from '../pages/Tentang.vue';

const routes = [
  { path: '/', component: Beranda },
  { path: '/penambangan-data', component: PenambanganData },
  { path: '/sains-data', component: SainsData },
  { path: '/analisis-data', component: AnalisisData },
  { path: '/proyek', component: Proyek },
  { path: '/prediksi', component: Prediksi },
  { path: '/chatbot', component: Chatbot },
  { path: '/tentang', component: Tentang },
];

const router = createRouter({
  history: createWebHistory(),
  routes,
});

export default router;
